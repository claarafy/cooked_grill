<!-- Start featured -->
<div class="featured">
  <div class="container">
    <div class="row form-wrapper">
      <div class="col-md-10 col-sm-12">
        <%= image_tag("img/slider/jssor/main_image.jpg", class:"img-responsive") %>
      </div>
      <div class="main-form">
        <h3>Make Extra Money <span>Become a Home Cook</span></h3>
        <p>
          It's finally here!
          Now you can share your culinary skills with your community and make extra money at the same time.
          Cook when you want, charge what you want. <br />Sign up for a Cook Account. It's free.
        </p>



        <%= form_for(resource, as: resource_name, url: registration_path(resource_name),  html: { class: "form-horizontal"}) do |f| %>
          <%= devise_error_messages! %>
          <div class="form-group">
            <div class="col-xs-6">
              <%= f.label "Name" %>
              <%= f.text_field :first_name, autofocus: true, class: "form-control form-block", autocomplete: "first_name", :required => true, placeholder: "Enter name" %>
              <!-- <input type="text" class="form-control form-block" placeholder="Enter name"> -->
            </div>
            <div class="col-xs-6">
              <%= f.label "Zip Code" %>
              <%= f.text_field :zip, class: "form-control form-block", autocomplete: "zip", :required => true, placeholder: "Enter zip code" %>
              <!-- <label>Zip :</label>
              <input type="text" class="form-control form-block" placeholder="Enter zip code"> -->
            </div>
          </div>
        <div class="form-group">
          <div class="col-xs-6">
            <%= f.label "Email" %>
            <%= f.email_field :email, class: "form-control form-block", autocomplete: "email", :required => true, placeholder: "Enter email" %>
            <!-- <label>Email :</label>
            <input type="text" class="form-control form-block" placeholder="Enter email"> -->
          </div>
          <div class="col-xs-6">
            <%= f.label :password %>
            <%= f.password_field :password, autocomplete: "off", class: "form-control form-block", placeholder: "Password (6 characters minimum)"%>
            <!-- <label>Password :</label>
            <input type="text" class="form-control form-block" placeholder="Enter password "> -->
          </div>
        </div>
        <!-- <div class="form-group">
          <div class="col-lg-12">
            <label>Confirm Password :</label>
            <input type="text" class="form-control form-block" placeholder="Re-enter the password">
          </div>
        </div> -->
        <div class="form-group">
          <div class="col-lg-12">
            <%= f.submit "Sign Up", class: "btn btn-primary btn-bavel" %>
            <!-- <button class="btn btn-primary btn-bavel">Sign Up</button> -->
          </div>
        </div>
      <% end %>




    </div>
  </div>
</div>
</div>
